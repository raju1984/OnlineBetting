<style>
    @@media (min-width: 320px) and (max-width: 480px) {
        .logo-res {
            width: 215px !important;
        }

        .counter-area-inner {
            margin-top: 80px !important;
        }

        .navbar-collapse {
            padding-left: 20px;
        }

        .nav-link {
            display: block;
            padding: 0rem 1rem;
        }

        ul.navbar-nav.mr-auto {
            margin-left: 0px !important;
        }
    }
