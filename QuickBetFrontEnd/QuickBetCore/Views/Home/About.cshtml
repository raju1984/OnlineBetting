        width: 250px;
    }

    ul.navbar-nav.mr-auto {
        margin-left: 50px;
    }

    .header-top-area-inner {
        width: 20%;
        float: left;
    }

    .header-wrapper {
        position: relative;
        left: 5%;
    }

    .header-section {
        background: #ffffff;
        padding: 10px 0;
    }

    .header-wrapper .menu li a {
        color: #4c4a4a;
        font-size: 16px;
        font-weight: 600;
